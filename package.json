    "@vue/cli-plugin-babel": "~4.5.2",
    "@vue/cli-plugin-eslint": "~4.5.2",
    "@storybook/vue": "^6.1.11",
    "@vue/cli-plugin-typescript": "~4.5.0",
    "@vue/cli-plugin-unit-jest": "~4.5.2",
    "@vue/cli-service": "~4.5.0",
    "babel-loader": "^8.2.2",
    "babel-preset-vue": "~2.0.2",
    "chromatic": "^5.4.0",
    "lint-staged": "^9.5.0",
